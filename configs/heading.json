 {
    "workbench.colorTheme": "GitHub Dark Default",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "psi-header.config": {
        "forceToTop": true,
        "blankLinesAfter": 1,
        "license": "BSD-3-Clause",
        "author": "John James",
        "initials": "J2",
        "authorEmail": "john.james@nov8.ai",
        "company": "nov8.ai",
        "creationDateZero": "asIs"
    },
    "psi-header.changes-tracking": {
        "isActive": true,
        "modAuthor": "Modified By: ",
        "modDate": "Date Modified: ",
        "modDateFormat": "MM/dd/yyyy hh:nn:ss",
        "autoHeader": "autoSave",
        "enforceHeader": true,
        "exclude": [
            "markdown",
            "json",
            "jsonc"
        ]        
    },
    "psi-header.lang-config": [
        {
			"language": "python",
			"begin": "#==============================================================================#",
			"prefix": "# ",
            "suffix": "#",
            "lineLength": 80,
			"end": "#==============================================================================#",
			"beforeHeader": [
				"#!/usr/bin/env python3",
				"# -*- coding:utf-8 -*-"
			]            
        }
    ],
	"psi-header.variables": [
		["project", "Predict-FDA"],
        ["language", "Python 3.9.5"],
        ["version", "0.1.0"],
        ["year", "2021"],
        ["url", "https://github.com/john-james-sf/predict-fda"]
	],    
    "psi-header.templates": [
        {
            "language": "python",
            "template": [
                "Project  : <<project>>",
                "Version  : <<version>>",
                "File     : <<filename>>",
                "Language : <<language>>",
                "-----------------------------------------------------------------------------",
                "Author   : <<author>>",
                "Company  : <<company>>",
                "Email    : <<authorEmail>>",
                "URL      : <<url>>",
                "-----------------------------------------------------------------------------",
                "Created  : <<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
                "Modified : <<dateformat('dddd, MMMM Do YYYY, h:mm:ss a')>>",
                "Modifier : <<author>> (<<authorEmail>>)",
                "-----------------------------------------------------------------------------",
                "License  : <<licensename>>",
                "Copyright: (c) <<year>> <<company>>"
            ]
        }
    ]
}